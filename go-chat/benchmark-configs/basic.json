{
  "test_name": "basic_performance",
  "description": "Basic performance baseline test",
  "server_startup_timeout": 10,
  "tests": {
    "connection_test": {
      "enabled": true,
      "target_connections": 5000,
      "batch_size": 50,
      "connection_timeout": 2.0,
      "failure_threshold": 0.3
    },
    "message_test": {
      "enabled": true,
      "target_multiplier": 100,
      "batch_size": 1000,
      "message_size_multiplier": 10,
      "error_threshold": 100
    },
    "endurance_test": {
      "enabled": false
    }
  },
  "reporting": {
    "progress_interval": 1000,
    "save_raw_data": true,
    "generate_charts": false
  }
}
